{
  "author": "intuit-svc <opensource-svc@intuit.com>",
  "baseBranch": "main",
  "versionBranches": true,
  "noVersionPrefix": true,
  "owner": "player-ui",
  "repo": "player",
  "plugins": [
    [
      "all-contributors",
      "version-file",
      {
        "versionFile": "./VERSION",
        "publishScript": "./scripts/release.sh",
        "publishScriptReleaseTypeArgs": {
          "next": [
            "next"
          ]
        }
      }
    ],
    [
      "omit-release-notes",
      {
        "labels": "release"
      }
    ],
    "./scripts/delete-old-prerelease.js",
    "./scripts/next-changelogs.js",
    "./scripts/version-comment.js",
    "./scripts/docs/release-docs.js"
  ]
}
